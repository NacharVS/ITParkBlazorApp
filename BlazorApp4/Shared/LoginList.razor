

<div class="table-responsive">
<table class="table table-striped">
    <thead>
        <tr>
            <th>Login</th>
            <th>Surname</th>
            <th>Name</th>
            <th>PhoneNumber</th>
        </tr>
    </thead>
    <tbody>
        @if (Logins == null)
        {
            <tr>
                <td> Loading....</td>
            </tr>
        }
        else
        {
            @foreach (var item in Logins)
            {
                <tr>
                    <td>@item.Login</td>
                    <td>@item.Surname</td>
                    <td>@item.Name</td>
                    <td>@item.PhoneNumber</td>
                </tr>
            }
        }

    </tbody>
</table>
</div>

@code {
    [Parameter]
    public List<User> Logins { get; set; }
}
